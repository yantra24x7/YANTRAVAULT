    let ShiftService = class ShiftService {
      constructor(token, http) {
        this.token = token;
        this.http = http;
        this.tenantId = this.token.getTenantID();
      }

      shift(tenantId) {
        return this.http.get('shifts?tenant_id=' + tenantId);
      }

      shifttransaction(shift_id) {
        return this.http.get('shifttransactions?shift_id=' + shift_id);
      }

      edit(id, val) {
        return this.http.put('shifts/' + id, val);
      }

      delete_row(id) {
        return this.http.delete('shifttransactions/' + id);
      }

      shift_edit(id, val) {
        return this.http.put('shifttransactions/' + id, val);
      }

      post(value) {
        return this.http.post('shifttransactions', value);
      }

    };

    ShiftService.ctorParameters = () => [{
      type: _Service_core_authentication_token_service__WEBPACK_IMPORTED_MODULE_2__["TokenService"]
